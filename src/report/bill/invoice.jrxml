	<subDataset name="ItemListByOrderId" whenResourceMissingType="Error" uuid="b1cd5bc1-b28c-4961-8399-a5f28be8eda3">
		<queryString>
			<![CDATA[SELECT
     order_item_list.quantity*menu.retail_price AS total_amount,
     order_item_list."menu_id",
     menu."menu_name",
     order_item_list."quantity",
     item_unit."unit_name",
     menu."retail_price"
FROM
     "order_item_list" order_item_list INNER JOIN "menu" menu ON order_item_list."menu_id" = menu."menu_id"
     INNER JOIN "item_unit" item_unit ON menu."unit_id" = item_unit."unit_id"
WHERE
     order_id IN (52)]]>
		</queryString>
		<field name="total_amount" class="java.math.BigDecimal"/>
		<field name="menu_id" class="java.lang.String"/>
		<field name="menu_name" class="java.lang.String"/>
		<field name="quantity" class="java.math.BigDecimal"/>
		<field name="unit_name" class="java.lang.String"/>
		<field name="retail_price" class="java.math.BigDecimal"/>
		<group name="menu_name">
			<groupExpression><![CDATA[$F{menu_name}]]></groupExpression>
		</group>
	</subDataset>
